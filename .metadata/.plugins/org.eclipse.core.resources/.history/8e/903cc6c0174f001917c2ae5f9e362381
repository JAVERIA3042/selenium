
import org.junit.After;
import org.junit.Before;
import org.junit.Test;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class ddf1 {
	WebDriver myDriver;
	
	@Before
	public void myBefore() {
		
		System.out.println("Before");
		//prepare Test
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Javeria Khaliq\\eclipse-workspace\\Selenium\\chromedriver.exe");
		myDriver = new ChromeDriver();
		myDriver.navigate().to("https://www.mortgagecalculator.org/");
		myDriver.manage().window().maximize();
	}

	@Test 
	public void mytest() {
		
		System.out.println("Test1");
		
		
		//myDriver.findElement(By.id("homeval")).sendKeys("4120000");
		myDriver.findElement(By.name("param[homevalue]")).sendKeys("412000");
		myDriver.findElement(By.name("param[downpayment]")).sendKeys("112000");
		myDriver.findElement(By.id("loanamt")).sendKeys("300000");
		myDriver.findElement(By.name("param[interest_rate]")).sendKeys("4");
		
		
		//Calculate the Value
		myDriver.findElement(By.name("cal")).click();
		

		
	}
	
	@After
	public void myafter() {
	
		System.out.println("My Test Successfully");
		//myDriver.close();
		//myDriver.quit();
	}

}
